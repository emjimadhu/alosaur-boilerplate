{
  "name": "alosaur-boilerplate",
  "version": "1.0.0",
  "description": "Alosaur Boilerplate",
  "scripts": {
    "start": "deno run -A --importmap=imports.json --config ./tsconfig.json app.ts",
    "start:dev": "denon",
    "format": "deno fmt \"**/**/*.ts\" \"app.ts\" --prettierrc=prettierrc.json",
    "docker:build": "docker build -t alosaur-boilerplate .",
    "docker:run": "docker run -p 8000:8000 alosaur-boilerplate"
  },
